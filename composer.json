{
  "name": "vendidero/ups-for-shiptastic",
  "description": "The Shiptastic UPS implementation.",
  "homepage": "https://github.com/vendidero/ups-for-shiptastic",
  "license": "GPL-3.0-or-later",
  "type": "wordpress-plugin",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "automattic/jetpack-autoloader": "2.11.18",
    "setasign/fpdf": "^1.8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0",
    "yoast/phpunit-polyfills": "^3.0",
    "woocommerce/woocommerce-sniffs": "^1.0.0"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": true,
    "platform": {
      "php": "7.4"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "composer dump-autoload"
    ],
    "post-update-cmd": [
      "composer dump-autoload"
    ],
    "phpcs": [
      "phpcs -s -p"
    ],
    "phpcs-pre-commit": [
      "phpcs -s -p -n"
    ],
    "phpcbf": [
      "phpcbf -p"
    ]
  },
  "autoload": {
    "psr-4": {
      "Vendidero\\Shiptastic\\UPS\\": "src/"
    }
  }
}